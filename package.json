{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:thinktwice13/fcc-vue-challenge.git",
  "author": "Mario K",
  "license": "MIT",
  "engines": {
    "npm": "5.3.x",
    "node": "8.x.x"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "express": "^4.15.4"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.1"
  },
  "scripts": {
    "heroku-postbuild": "npm install  && cd client && npm install && npm install --only=dev && npm run build",
    "start": "node index",
    "server": "nodemon index",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ]
}
